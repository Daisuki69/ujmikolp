<?xml version="1.0" encoding="utf-8"?>
<dOYHB6.lN8Dz0.yf3D09 xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/loader"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/maya_fragment_kyc_initial_loading"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="?attr/actionBarSize">
        <ImageView
            android:id="@+id/image_view_rockon"
            android:layout_width="@dimen/maya_kyc_rockon_image_size"
            android:layout_height="@dimen/maya_kyc_rockon_image_size"
            android:src="0x7f0803d9"
            android:importantForAccessibility="no"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.paymaya.mayaui.common.view.widget.MultiColorTextView
            android:id="@+id/text_view_header"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/maya_margin_large"
            android:layout_marginTop="@dimen/maya_margin_normal"
            android:layout_marginRight="@dimen/maya_margin_large"
            android:text="@string/maya_kyc_block_submitted_header"
            android:layout_marginHorizontal="@dimen/maya_margin_large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_rockon"
            app:mctvSecondaryTextColor="?attr/mayaColorAccent"
            app:mctvSecondaryTextHighlightSpan="@string/maya_kyc_block_submitted_header_highlighted_phrase"
            style="@style/JekoBoldH2.Primary"/>
        <TextView
            android:id="@+id/text_view_header_subtext"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/maya_margin_normal"
            android:text="@string/maya_kyc_block_submitted_header_subtext"
            app:layout_constraintEnd_toEndOf="@+id/text_view_header"
            app:layout_constraintStart_toStartOf="@+id/text_view_header"
            app:layout_constraintTop_toBottomOf="@+id/text_view_header"
            style="@style/JekoSemiBoldLongRead.Tertiary"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_layout_information_box"
            android:background="@drawable/maya_bg_drawable_gray_rounded_corner_border"
            android:padding="@dimen/maya_padding_normal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/maya_margin_large"
            android:layout_marginTop="@dimen/maya_margin_large"
            android:layout_marginRight="@dimen/maya_margin_large"
            android:layout_marginHorizontal="@dimen/maya_margin_large"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_view_header_subtext">
            <ImageView
                android:id="@+id/image_view_warning_icon"
                android:layout_width="@dimen/maya_kyc_information_icon_size"
                android:layout_height="@dimen/maya_kyc_information_icon_size"
                android:src="@drawable/maya_ic_information_warning_badge"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:id="@+id/text_view_account_status_header"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:text="@string/maya_kyc_block_submitted_account_status_header"
                android:layout_marginStart="@dimen/maya_margin_extra_small"
                app:layout_constraintBottom_toBottomOf="@+id/image_view_warning_icon"
                app:layout_constraintStart_toEndOf="@+id/image_view_warning_icon"
                app:layout_constraintTop_toTopOf="@+id/image_view_warning_icon"
                style="@style/CerebriMediumSmallText.Primary"/>
            <com.paymaya.mayaui.common.view.widget.MultiColorTextView
                android:id="@+id/text_view_account_status_description_under_review"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_extra_small"
                android:text="@string/maya_kyc_block_submitted_account_status_subtext_line_under_review"
                android:layout_marginEnd="@dimen/maya_margin_normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/text_view_account_status_header"
                app:layout_constraintTop_toBottomOf="@+id/text_view_account_status_header"
                app:mctvSecondaryTextColor="@color/maya_system_warning"
                app:mctvSecondaryTextHighlightSpan="@string/maya_kyc_block_submitted_account_status_subtext_line_under_review_highlighted_phrase"
                style="@style/CerebriMediumSmallText.Tertiary"/>
            <TextView
                android:id="@+id/text_view_account_status_description"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_extra_small"
                android:text="@string/maya_kyc_block_submitted_account_status_subtext_line_description"
                android:layout_marginEnd="@dimen/maya_margin_normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/text_view_account_status_header"
                app:layout_constraintTop_toBottomOf="@+id/text_view_account_status_description_under_review"
                style="@style/CerebriMediumSmallText.Tertiary"/>
            <com.paymaya.mayaui.common.view.widget.MultiColorTextView
                android:id="@+id/text_view_account_status_reference_number"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_extra_small"
                android:text="@string/maya_kyc_block_submitted_account_status_subtext_line_reference_number"
                android:layout_marginEnd="@dimen/maya_margin_normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/text_view_account_status_header"
                app:layout_constraintTop_toBottomOf="@+id/text_view_account_status_description"
                app:mctvSecondaryTextColor="@color/maya_shades_of_grey_5"
                app:mctvSecondaryTextHighlightSpan="@string/maya_kyc_block_submitted_account_status_subtext_line_reference_number"
                style="@style/CerebriMediumSmallText.Tertiary"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <Button
            android:id="@+id/button_logout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/maya_margin_large"
            android:layout_marginRight="@dimen/maya_margin_large"
            android:layout_marginBottom="@dimen/maya_margin_12"
            android:text="@string/maya_label_log_out"
            android:layout_marginHorizontal="@dimen/maya_margin_large"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            style="@style/MayaButtonPrimaryLarge"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</dOYHB6.lN8Dz0.yf3D09>
