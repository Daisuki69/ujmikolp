<?xml version="1.0" encoding="utf-8"?>
<dOYHB6.lN8Dz0.zvMGC1 xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/txy_config_dialog_bg"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <Button
        android:textSize="13sp"
        android:textColor="@color/txy_white"
        android:id="@+id/txy_ocr_dialog_confirm_button"
        android:background="@drawable/txy_huiyan_default_button_bg"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="10dp"
        android:text="@string/txy_huiyan_confirm_txt"
        android:layout_below="@+id/txy_ocr_dialog_confirm_checked_box"
        android:layout_alignParentRight="true"/>
    <CheckBox
        android:textSize="14sp"
        android:textStyle="bold"
        android:gravity="center_vertical"
        android:id="@+id/txy_ocr_dialog_confirm_checked_box"
        android:padding="3dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:button="@drawable/txy_huiyan_checkbox_bg"
        android:text="@string/txy_huiyan_confirm_dialog_checkbox"
        android:layout_below="@+id/txy_ocr_dialog_confirm_tv"/>
    <TextView
        android:textSize="13sp"
        android:textColor="@color/txy_black"
        android:id="@+id/txy_ocr_dialog_confirm_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="15dp"
        android:text="@string/txy_huiyan_confirm_txt"
        android:layout_marginHorizontal="15dp"/>
</dOYHB6.lN8Dz0.zvMGC1>
