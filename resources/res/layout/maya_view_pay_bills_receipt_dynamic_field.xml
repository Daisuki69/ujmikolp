<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <dOYHB6.lN8Dz0.d8NVX3
        android:orientation="horizontal"
        android:id="@+id/dynamic_field_contents_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:id="@+id/dynamic_field_label_text_view"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            style="@style/JekoSemiBoldSmallText.Tertiary"/>
        <TextView
            android:id="@+id/dynamic_field_value_text_view"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            style="@style/JekoSemiBoldLink.Primary"/>
    </dOYHB6.lN8Dz0.d8NVX3>
    <View
        android:id="@+id/dynamic_field_divider_view"
        android:layout_marginTop="@dimen/pma_margin_normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dynamic_field_contents_linear_layout"
        style="@style/MayaListItemDivider"/>
    <Space
        android:id="@+id/account_number_divider_bottom_space"
        android:layout_width="match_parent"
        android:layout_height="@dimen/pma_margin_normal"
        app:layout_constraintTop_toBottomOf="@+id/dynamic_field_divider_view"/>
</androidx.constraintlayout.widget.ConstraintLayout>
