<?xml version="1.0" encoding="utf-8"?>
<dOYHB6.lN8Dz0.d8NVX3 xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:orientation="vertical"
    android:background="@drawable/maya_background_card_gray"
    android:paddingLeft="@dimen/maya_padding_normal"
    android:paddingTop="@dimen/maya_padding_medium"
    android:paddingRight="@dimen/maya_padding_normal"
    android:paddingBottom="@dimen/maya_padding_medium"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/maya_kyc_single_selection_card_min_height"
    android:paddingHorizontal="@dimen/maya_padding_normal"
    android:paddingVertical="@dimen/maya_padding_medium">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/text_view_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:maxLines="4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/image_view_checkmark"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/JekoSemiBoldTextBody.Primary"/>
        <ImageView
            android:id="@+id/image_view_checkmark"
            android:visibility="invisible"
            android:layout_width="@dimen/maya_kyc_accepted_id_view_holder_checkmark_icon_size"
            android:layout_height="@dimen/maya_kyc_accepted_id_view_holder_checkmark_icon_size"
            android:src="@drawable/maya_ic_check_green"
            android:contentDescription="@string/maya_label_check_deals"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <dOYHB6.lN8Dz0.d8NVX3
        android:orientation="horizontal"
        android:id="@+id/linear_container_warning"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_small"
        android:layout_marginBottom="@dimen/maya_margin_extra_small">
        <ImageView
            android:id="@+id/image_warning_icon"
            android:visibility="gone"
            android:layout_width="@dimen/maya_icon_size_length_small"
            android:layout_height="@dimen/maya_icon_size_length_small"
            android:src="@drawable/maya_ic_information_error_badge"
            android:contentDescription="@null"
            android:layout_marginEnd="@dimen/maya_margin_6"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_view_item_name"
            app:tint="@color/pumpkin_orange"/>
        <TextView
            android:id="@+id/text_view_sub_item_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/image_view_checkmark"
            app:layout_constraintStart_toEndOf="@+id/image_warning_icon"
            app:layout_constraintTop_toBottomOf="@+id/text_view_item_name"
            style="@style/CerebriBoldSmallText.Warning"/>
    </dOYHB6.lN8Dz0.d8NVX3>
</dOYHB6.lN8Dz0.d8NVX3>
