<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_card_view_pma_view_holder_favorite_card_loading"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/favourite_loading_second"
        android:layout_width="@dimen/maya_favorite_loading_box_size"
        android:layout_height="@dimen/maya_favorite_loading_box_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/favourite_loading_third"
        android:layout_width="@dimen/maya_favorite_loading_cell_line_width"
        android:layout_height="@dimen/maya_favorite_loading_cell_line_height"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginTop="@dimen/maya_margin_10"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintEnd_toEndOf="@+id/favourite_loading_second"
        app:layout_constraintStart_toStartOf="@+id/favourite_loading_second"
        app:layout_constraintTop_toBottomOf="@+id/favourite_loading_second"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/favourite_loading_fourth"
        android:layout_width="@dimen/maya_favorite_loading_cell_line_width"
        android:layout_height="@dimen/maya_favorite_loading_cell_line_height"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginTop="@dimen/maya_margin_5"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintEnd_toEndOf="@+id/favourite_loading_second"
        app:layout_constraintStart_toStartOf="@+id/favourite_loading_second"
        app:layout_constraintTop_toBottomOf="@+id/favourite_loading_third"
        layout="@layout/maya_view_shimmer_rounded"/>
</androidx.constraintlayout.widget.ConstraintLayout>
