            app:layout_constraintVertical_chainStyle="packed"/>
        <TextView
            android:gravity="center_horizontal"
            android:id="@+id/title_text_pma_fragment_unsupported_web"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/pma_margin_extra_large"
            android:layout_marginTop="@dimen/pma_margin_40dp"
            android:layout_marginRight="@dimen/pma_margin_extra_large"
            android:text="@string/pma_label_unsupported_web_title"
            android:fontFamily="@string/pma_font_family_roboto_medium"
            app:layout_constraintBottom_toTopOf="@+id/subtitle_text_pma_fragment_unsupported_web"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image_view_pma_fragment_unsupported_web"
            style="@style/BodyDarkBold"/>
        <TextView
            android:gravity="center_horizontal"
            android:id="@+id/subtitle_text_pma_fragment_unsupported_web"
            android:padding="@dimen/pma_padding_small"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/pma_margin_extra_large"
            android:layout_marginTop="@dimen/pma_margin_10dp"
            android:layout_marginRight="@dimen/pma_margin_extra_large"
            android:text="@string/pma_label_unsupported_web_subtitle"
            android:lineSpacingExtra="@dimen/pma_line_spacing_2sp"
            app:layout_constraintBottom_toTopOf="@+id/download_button_pma_fragment_unsupported_web"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/title_text_pma_fragment_unsupported_web"
            style="@style/Body2DarkLight"/>
        <TextView
            android:textSize="@dimen/pma_text_size_body"
            android:gravity="center"
            android:id="@+id/download_button_pma_fragment_unsupported_web"
            android:layout_width="0dp"
            android:layout_height="@dimen/pma_button_min_height"
            android:layout_marginTop="@dimen/pma_margin_normal"
            android:layout_marginBottom="@dimen/pma_margin_extra_large"
            android:text="@string/pma_label_unsupported_web_download_chrome"
            android:fontFamily="@string/pma_font_family_roboto_medium"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subtitle_text_pma_fragment_unsupported_web"
            app:layout_constraintWidth_percent="0.6"
            style="?attr/secondaryButton"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
