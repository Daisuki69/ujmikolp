                style="@style/JekoBoldH4.Primary"/>
            <TextView
                android:id="@+id/text_view_form_sub_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_small"
                android:text="@string/maya_kyc_personal_info_confirm_submission_sub_header"
                style="@style/CerebriBookLink.Secondary"/>
            <View
                android:id="@+id/view_personal_details_divider"
