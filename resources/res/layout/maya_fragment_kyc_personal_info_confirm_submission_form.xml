                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_kyc_complete_your_maya_experience_data_privacy_header"
                    style="@style/CerebriMediumLink.Primary"/>
                <com.paymaya.common.widget.HtmlTextView
                    android:textColorLink="@color/pma_primary_100_base"
                    android:id="@+id/text_view_data_privacy_description"
