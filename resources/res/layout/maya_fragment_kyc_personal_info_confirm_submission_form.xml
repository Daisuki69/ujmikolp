            <include
                android:id="@+id/layout_form_personal_details"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                layout="@layout/maya_fragment_kyc_personal_info_confirm_submission_personal_details_section"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                style="@style/KycPersonalInformationDivider"/>
            <include
                android:id="@+id/layout_form_work_details"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                layout="@layout/maya_fragment_kyc_personal_info_confirm_submission_work_details_section"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                style="@style/KycPersonalInformationDivider"/>
            <include
