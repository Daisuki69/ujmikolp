<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="@dimen/pma_padding_small"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/arrow_back_image_view"
        android:background="?attr/selectableItemBackground"
        android:padding="@dimen/maya_padding_small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginBottom="@dimen/maya_margin_normal"
        android:src="@drawable/maya_ic_profile_back_arrow"
        android:tint="?attr/mayaColorIconTint"
        android:layout_marginStart="@dimen/maya_margin_20"
        android:layout_marginVertical="@dimen/maya_margin_normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/account_status_text_view"
        android:paddingLeft="@dimen/maya_padding_normal"
        android:paddingTop="@dimen/maya_padding_small"
        android:paddingRight="@dimen/maya_padding_normal"
        android:paddingBottom="@dimen/maya_padding_small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_20"
        android:layout_marginBottom="@dimen/maya_margin_20"
        android:drawablePadding="@dimen/pma_padding_extra_small"
        android:layout_marginVertical="@dimen/maya_margin_20"
        android:paddingHorizontal="@dimen/maya_padding_normal"
        android:paddingVertical="@dimen/maya_padding_small"
        app:drawableLeftCompat="@drawable/maya_ic_profile_basic_account"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/JekoBoldSmallText.Primary"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/qr_image_view"
        android:background="?attr/selectableItemBackground"
        android:padding="@dimen/maya_padding_small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginBottom="@dimen/maya_margin_normal"
        android:src="@drawable/maya_ic_profile_qr"
        android:tint="?attr/mayaColorIconTint"
        android:layout_marginEnd="@dimen/maya_margin_20"
        android:layout_marginVertical="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <dOYHB6.lN8Dz0.yf3D09
        android:id="@+id/frame_layout_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/maya_margin_normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arrow_back_image_view"/>
</androidx.constraintlayout.widget.ConstraintLayout>
