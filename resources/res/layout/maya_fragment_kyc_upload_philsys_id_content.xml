<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView
        android:id="@+id/nested_scroll_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/maya_margin_small"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/maya_margin_large"
            android:layout_marginRight="@dimen/maya_margin_large"
            android:layout_marginHorizontal="@dimen/maya_margin_large">
            <TextView
                android:id="@+id/text_view_upload_philsys_header"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_extra_large"
                android:text="@string/maya_kyc_upload_philsys_header"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/JekoBoldH4.Primary"/>
            <TextView
                android:id="@+id/text_view_upload_philsys_sub_header"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_small"
                android:text="@string/maya_kyc_upload_philsys_header_subtext"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/text_view_upload_philsys_header"
                style="@style/CerebriBookLink.Secondary"/>
            <dOYHB6.lN8Dz0.d8NVX3
                android:orientation="vertical"
                android:id="@+id/linear_layout_philsys_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/text_view_upload_philsys_sub_header">
                <TextView
                    android:id="@+id/text_view_philsys_front_section_header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_extra_large"
                    android:text="@string/maya_kyc_upload_philsys_section_front_title"
                    style="@style/CerebriMediumTextBody.Primary"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraint_layout_philsys_front_id_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_extra_normal">
                    <dOYHB6.lN8Dz0.d8NVX3
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/linear_layout_philsys_front_id_container"
                        android:background="@drawable/maya_bg_selector_rectangle_with_dashed_stroke"
                        android:paddingTop="@dimen/maya_padding_20dp"
                        android:paddingBottom="@dimen/maya_padding_25dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">
                        <TextView
                            android:textSize="@dimen/maya_line_height_24sp"
                            android:id="@+id/text_view_document_facing_up_emoji_front"
                            android:padding="@dimen/maya_padding_2x_small"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/maya_kyc_upload_philsys_upload_emoji"/>
                        <ProgressBar
                            android:id="@+id/progress_bar_upload_philsys_front"
                            android:visibility="gone"
                            android:layout_width="@dimen/maya_kyc_upload_progress_bar_width"
                            android:layout_height="@dimen/maya_kyc_upload_progress_bar_height"
                            style="?android:attr/progressBarStyle"/>
                        <TextView
                            android:gravity="center_horizontal"
                            android:id="@+id/text_view_philsys_front_upload_file_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/maya_kyc_upload_philsys_section_front_upload_title"
                            style="@style/CerebriMediumLongRead.Primary"/>
                        <TextView
                            android:gravity="center_horizontal"
                            android:id="@+id/text_view_philsys_front_upload_file_description"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/maya_kyc_upload_philsys_section_front_upload_description"
                            style="@style/CerebriBookSmallText.Secondary"/>
                        <Button
                            android:enabled="true"
                            android:id="@+id/button_philsys_front_add_file"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/maya_kyc_upload_philsys_add_file_height"
                            android:layout_marginTop="@dimen/maya_margin_12"
                            android:maxWidth="@dimen/maya_kyc_upload_philsys_add_file_width"
                            android:text="@string/maya_kyc_upload_philsys_section_front_upload_cta"
                            android:drawableStart="@drawable/pma_ic_add"
                            android:paddingStart="@dimen/maya_padding_normal"
                            android:paddingEnd="@dimen/maya_padding_normal"
                            style="@style/MayaButtonBlackSmall"/>
                    </dOYHB6.lN8Dz0.d8NVX3>
                    <ImageView
                        android:id="@+id/image_view_philsys_id_front"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <Button
                    android:enabled="true"
                    android:textAppearance="@style/JekoBoldLink.Primary"
                    android:gravity="center"
                    android:id="@+id/button_replace_front_photo"
                    android:paddingTop="@dimen/maya_padding_normal"
                    android:paddingBottom="@dimen/maya_padding_normal"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_12"
                    android:text="@string/maya_kyc_upload_philsys_replace_photo_front"
                    android:drawableStart="@drawable/maya_ic_camera_primary"
                    android:paddingStart="@dimen/maya_padding_2x_large"
                    android:paddingEnd="@dimen/maya_padding_2x_large"
                    style="@style/MayaButtonSecondarySmall"/>
                <TextView
                    android:id="@+id/text_view_philsys_back_section_header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_extra_large"
                    android:text="@string/maya_kyc_upload_philsys_section_back_title"
                    style="@style/CerebriMediumTextBody.Primary"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraint_layout_philsys_back_id_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_extra_normal">
                    <dOYHB6.lN8Dz0.d8NVX3
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/linear_layout_philsys_back_id_container"
                        android:background="@drawable/maya_bg_selector_rectangle_with_dashed_stroke"
                        android:paddingTop="@dimen/maya_padding_20dp"
                        android:paddingBottom="@dimen/maya_padding_25dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">
                        <TextView
                            android:textSize="@dimen/maya_line_height_24sp"
                            android:id="@+id/text_view_document_facing_up_emoji_back"
                            android:padding="@dimen/maya_padding_2x_small"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/maya_kyc_upload_philsys_upload_emoji"/>
                        <ProgressBar
                            android:id="@+id/progress_bar_upload_philsys_back"
                            android:visibility="gone"
                            android:layout_width="@dimen/maya_kyc_upload_progress_bar_width"
                            android:layout_height="@dimen/maya_kyc_upload_progress_bar_height"
                            style="?android:attr/progressBarStyle"/>
                        <TextView
                            android:gravity="center_horizontal"
                            android:id="@+id/text_view_philsys_back_upload_file_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/maya_kyc_upload_philsys_section_back_upload_title"
                            style="@style/CerebriMediumLongRead.Primary"/>
                        <TextView
                            android:gravity="center_horizontal"
                            android:id="@+id/text_view_philsys_back_upload_file_description"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/maya_kyc_upload_philsys_section_back_upload_description"
                            style="@style/CerebriBookSmallText.Secondary"/>
                        <Button
                            android:enabled="true"
                            android:id="@+id/button_philsys_back_add_file"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/maya_kyc_upload_philsys_add_file_height"
                            android:layout_marginTop="@dimen/maya_margin_12"
                            android:maxWidth="@dimen/maya_kyc_upload_philsys_add_file_width"
                            android:text="@string/maya_kyc_upload_philsys_section_back_upload_cta"
                            android:drawableStart="@drawable/pma_ic_add"
                            android:paddingStart="@dimen/maya_padding_normal"
                            android:paddingEnd="@dimen/maya_padding_normal"
                            style="@style/MayaButtonBlackSmall"/>
                    </dOYHB6.lN8Dz0.d8NVX3>
                    <ImageView
                        android:id="@+id/image_view_philsys_id_back"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="fitCenter"
                        android:adjustViewBounds="true"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <Button
                    android:enabled="true"
                    android:textAppearance="@style/JekoBoldLink.Primary"
                    android:gravity="center"
                    android:id="@+id/button_replace_back_photo"
                    android:paddingTop="@dimen/maya_padding_normal"
                    android:paddingBottom="@dimen/maya_padding_normal"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_12"
                    android:text="@string/maya_kyc_upload_philsys_replace_photo_back"
                    android:drawableStart="@drawable/maya_ic_camera_primary"
                    android:paddingStart="@dimen/maya_padding_2x_large"
                    android:paddingEnd="@dimen/maya_padding_2x_large"
                    style="@style/MayaButtonSecondarySmall"/>
                <Button
                    android:enabled="false"
                    android:id="@+id/button_continue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_extra_large"
                    android:layout_marginBottom="@dimen/maya_margin_extra_large"
                    android:text="@string/maya_kyc_upload_philsys_continue"
                    android:layout_marginVertical="@dimen/maya_margin_extra_large"
                    style="@style/MayaButtonPrimaryLarge"/>
            </dOYHB6.lN8Dz0.d8NVX3>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
