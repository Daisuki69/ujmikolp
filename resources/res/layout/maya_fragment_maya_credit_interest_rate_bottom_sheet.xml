<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:id="@+id/top_sheet_line_view"
        android:background="@drawable/maya_bg_default_bottom_dialog_sheet"
        android:layout_width="@dimen/maya_bottom_sheet_line_width"
        android:layout_height="1dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/sheet_background_view"
        android:background="@drawable/maya_bg_default_bottom_dialog_sheet"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/maya_margin_6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top_sheet_line_view"
        app:layout_constraintVertical_bias="0"/>
    <androidx.constraintlayout.widget.Guideline
        android:orientation="vertical"
        android:id="@+id/left_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_begin="@dimen/maya_margin_large"/>
    <androidx.constraintlayout.widget.Guideline
        android:orientation="vertical"
        android:id="@+id/right_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_end="@dimen/maya_margin_large"/>
    <TextView
        android:id="@+id/title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_52"
        android:text="@string/maya_label_interest_rate_title"
        app:layout_constraintEnd_toEndOf="@+id/right_guideline"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/left_guideline"
        app:layout_constraintTop_toTopOf="@+id/sheet_background_view"
        style="@style/JekoBoldH5.Primary"/>
    <View
        android:id="@+id/interest_rate_table_view"
        android:background="@drawable/maya_bg_color_background_secondary_rounded"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/maya_margin_extra_large"
        app:layout_constraintBottom_toBottomOf="@+id/interest_rate_table_bottom_space"
        app:layout_constraintEnd_toEndOf="@+id/right_guideline"
        app:layout_constraintStart_toStartOf="@+id/left_guideline"
        app:layout_constraintTop_toBottomOf="@+id/title_text_view"/>
    <TextView
        android:id="@+id/coverage_period_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_coverage_period"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toTopOf="@+id/interest_rate_table_view"
        style="@style/JekoBoldLongRead.Primary"/>
    <TextView
        android:id="@+id/coverage_period_value_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_extra_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_coverage_period_value"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/coverage_period_title_text_view"
        style="@style/JekoRegularLongRead.DialogValue"/>
    <View
        android:id="@+id/coverage_period_divider_view"
        android:layout_width="0dp"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/coverage_period_value_text_view"
        style="@style/MayaContentViewDivider"/>
    <TextView
        android:id="@+id/used_limit_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_used_limit"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/coverage_period_divider_view"
        style="@style/JekoSemiBoldLongRead.Secondary"/>
    <TextView
        android:id="@+id/used_limit_value_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_used_limit_value"
        android:textAlignment="textEnd"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toEndOf="@+id/used_limit_title_text_view"
        app:layout_constraintTop_toTopOf="@+id/used_limit_title_text_view"
        style="@style/JekoRegularLongRead.DialogValue"/>
    <TextView
        android:id="@+id/service_fee_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_service_fee"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/used_limit_title_text_view"
        style="@style/JekoSemiBoldLongRead.Secondary"/>
    <TextView
        android:id="@+id/service_fee_value_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_service_fee_value"
        android:textAlignment="textEnd"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toEndOf="@+id/service_fee_title_text_view"
        app:layout_constraintTop_toTopOf="@+id/service_fee_title_text_view"
        style="@style/JekoRegularLongRead.DialogValue"/>
    <TextView
        android:id="@+id/service_fee_caption_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_2x_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_service_fee_caption"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/service_fee_title_text_view"
        style="@style/JekoRegularLongRead.Tertiary"/>
    <TextView
        android:id="@+id/stamp_tax_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_stamp_tax"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/service_fee_caption_text_view"
        style="@style/JekoSemiBoldLongRead.Secondary"/>
    <TextView
        android:id="@+id/stamp_tax_value_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_stamp_tax_value"
        android:textAlignment="textEnd"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toEndOf="@+id/stamp_tax_title_text_view"
        app:layout_constraintTop_toTopOf="@+id/stamp_tax_title_text_view"
        style="@style/JekoRegularLongRead.DialogValue"/>
    <TextView
        android:id="@+id/stamp_tax_caption_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_2x_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_stamp_tax_caption"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/stamp_tax_title_text_view"
        style="@style/JekoRegularLongRead.Tertiary"/>
    <View
        android:id="@+id/fees_divider_view"
        android:layout_width="0dp"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/stamp_tax_caption_text_view"
        style="@style/MayaContentViewDivider"/>
    <TextView
        android:id="@+id/total_bill_amount_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_total_bill_amount"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/fees_divider_view"
        style="@style/JekoBoldLongRead.Primary"/>
    <TextView
        android:id="@+id/total_bill_amount_value_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_interest_rate_total_bill_amount_value"
        android:textAlignment="textEnd"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toEndOf="@+id/total_bill_amount_title_text_view"
        app:layout_constraintTop_toTopOf="@+id/total_bill_amount_title_text_view"
        style="@style/JekoBoldLongRead.Primary"/>
    <Space
        android:id="@+id/interest_rate_table_bottom_space"
        android:layout_width="0dp"
        android:layout_height="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="@+id/interest_rate_table_view"
        app:layout_constraintStart_toStartOf="@+id/interest_rate_table_view"
        app:layout_constraintTop_toBottomOf="@+id/total_bill_amount_value_text_view"/>
    <Button
        android:id="@+id/done_button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_large"
        android:layout_marginBottom="@dimen/maya_margin_52"
        android:text="@string/maya_label_interest_rate_got_it"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/right_guideline"
        app:layout_constraintStart_toStartOf="@+id/left_guideline"
        app:layout_constraintTop_toBottomOf="@+id/interest_rate_table_bottom_space"
        style="@style/MayaButtonPrimaryLarge"/>
</androidx.constraintlayout.widget.ConstraintLayout>
