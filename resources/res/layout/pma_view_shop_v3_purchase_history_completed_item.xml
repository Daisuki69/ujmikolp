        app:constraint_referenced_ids="gaming_pin_background_view_pma_view_shop_v3_purchase_history_completed_item,         gaming_pin_code_label_text_view_pma_view_shop_v3_purchase_history_completed_item,         gaming_pin_code_value_text_view_pma_view_shop_v3_purchase_history_completed_item,         gaming_pin_copy_text_view_pma_view_shop_v3_purchase_history_completed_item"/>
