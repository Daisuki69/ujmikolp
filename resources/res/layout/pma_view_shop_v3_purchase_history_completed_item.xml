        app:layout_constraintLeft_toRightOf="@+id/gaming_pin_code_value_text_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintRight_toRightOf="@+id/gaming_pin_background_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintTop_toTopOf="@+id/gaming_pin_code_label_text_view_pma_view_shop_v3_purchase_history_completed_item"
        style="@style/CaptionDarkLightBold"/>
