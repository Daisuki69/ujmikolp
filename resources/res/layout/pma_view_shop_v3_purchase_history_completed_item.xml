        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/pma_margin_normal"
        app:layout_constraintBottom_toBottomOf="@+id/gaming_pin_code_value_text_view_pma_view_shop_v3_purchase_history_completed_item"
