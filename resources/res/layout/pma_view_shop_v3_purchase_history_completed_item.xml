        android:id="@+id/gaming_pin_code_value_text_view_pma_view_shop_v3_purchase_history_completed_item"
        android:paddingTop="@dimen/pma_margin_normal"
        android:paddingBottom="@dimen/pma_margin_normal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/pma_margin_small"
