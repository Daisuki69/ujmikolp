        android:paddingTop="@dimen/pma_padding_extra_small"
        android:paddingBottom="@dimen/pma_padding_extra_small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/pma_padding_medium"
        android:text="@string/pma_label_copy_all_caps"
