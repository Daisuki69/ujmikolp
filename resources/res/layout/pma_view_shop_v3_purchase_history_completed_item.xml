        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/pma_margin_normal"
        app:layout_constraintBottom_toBottomOf="@+id/treats_code_value_text_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintTop_toBottomOf="@+id/gaming_pin_background_view_pma_view_shop_v3_purchase_history_completed_item"/>
    <TextView
        android:gravity="center"
        android:id="@+id/treats_code_label_text_view_pma_view_shop_v3_purchase_history_completed_item"
        android:paddingTop="@dimen/pma_margin_normal"
        android:paddingBottom="@dimen/pma_margin_normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pma_margin_normal"
        android:layout_marginRight="@dimen/pma_margin_small"
        android:text="@string/pma_label_treat_code_c"
        app:layout_constraintLeft_toLeftOf="@+id/treats_background_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintRight_toLeftOf="@+id/treats_code_value_text_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintTop_toTopOf="@+id/treats_background_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintVertical_bias="0"
        style="@style/Body2DarkLight"/>
    <TextView
        android:textSize="@dimen/pma_text_size_body2"
        android:typeface="monospace"
        android:textStyle="bold"
        android:textColor="@color/dark"
        android:gravity="center"
        android:id="@+id/treats_code_value_text_view_pma_view_shop_v3_purchase_history_completed_item"
        android:paddingTop="@dimen/pma_margin_normal"
        android:paddingBottom="@dimen/pma_margin_normal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/pma_margin_normal"
        android:maxLines="3"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toRightOf="@+id/treats_code_label_text_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintRight_toRightOf="@+id/treats_background_view_pma_view_shop_v3_purchase_history_completed_item"
        app:layout_constraintTop_toTopOf="@+id/treats_code_label_text_view_pma_view_shop_v3_purchase_history_completed_item"/>
</androidx.constraintlayout.widget.ConstraintLayout>
