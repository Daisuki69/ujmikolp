        app:layout_constraintStart_toStartOf="@+id/view_billing_statement_container"
        app:layout_constraintTop_toBottomOf="@+id/button_billing_statement"/>
    <ImageView
        android:id="@+id/image_view_billing_statement_message_badge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_medium"
