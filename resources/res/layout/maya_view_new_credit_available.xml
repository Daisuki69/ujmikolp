        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_medium"
        android:text="@string/maya_label_billing_statement_message"
        android:layout_marginStart="@dimen/maya_margin_small"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/image_view_billing_statement_message_badge"
        app:layout_constraintTop_toBottomOf="@+id/view_bill_statement_bottom_space"
        style="@style/CerebriBookSmallText.Quaternary"/>
