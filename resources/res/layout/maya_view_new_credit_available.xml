        android:id="@+id/button_billing_statement_pay_now"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_medium"
        android:text="@string/maya_label_billing_statement_pay_now"
        android:layout_marginStart="@dimen/maya_margin_small"
        android:layout_marginEnd="@dimen/maya_margin_normal"
        app:layout_constraintBottom_toTopOf="@+id/view_bill_statement_bottom_space"
        app:layout_constraintEnd_toEndOf="@+id/view_billing_statement_container"
        app:layout_constraintStart_toStartOf="@+id/guideline_vertical_billing_statement"
        app:layout_constraintTop_toBottomOf="@+id/text_view_billing_statement_due_date_value"
