        android:background="@drawable/maya_bg_content_information_grey_2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintBottom_toBottomOf="@+id/view_pay_early_bottom_space"
        app:layout_constraintTop_toBottomOf="@+id/text_view_recent_transactions_label"/>
    <TextView
        android:id="@+id/text_view_credit_balance_this_period_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:text="@string/maya_label_pay_early_credit_balance_this_period_title"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintStart_toStartOf="@+id/view_pay_early_container"
        app:layout_constraintTop_toTopOf="@+id/view_pay_early_container"
        style="@style/JekoRegularSmallText.Primary"/>
    <TextView
        android:id="@+id/text_view_credit_balance_this_period_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_extra_small"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintStart_toStartOf="@+id/view_pay_early_container"
        app:layout_constraintTop_toBottomOf="@+id/text_view_credit_balance_this_period_label"
