<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.helper.widget.Flow
        android:id="@+id/flow_additional_docs_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_kyc_complete_personal_information_section_margin"
        app:constraint_referenced_ids="layout_confirm_personal_info_details_header,layout_confirm_personal_info_birth_certificate,layout_confirm_personal_info_parent_id,layout_confirm_personal_info_parental_consent"
        app:flow_maxElementsWrap="1"
        app:flow_verticalGap="@dimen/maya_margin_medium"
        app:flow_wrapMode="chain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <dOYHB6.lN8Dz0.d8NVX3
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/layout_confirm_personal_info_details_header"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:gravity="start"
            android:id="@+id/text_view_additional_docs"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/maya_kyc_personal_info_confirm_submission_additional_docs_header"
            android:layout_weight="1"
            style="@style/CerebriMediumTextBody.Primary"/>
        <TextView
            android:textColor="@drawable/maya_color_selector_content_links_accent"
            android:gravity="end"
            android:id="@+id/text_view_edit_additional_docs"
            android:focusable="true"
            android:clickable="true"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/maya_kyc_personal_info_confirm_submission_edit_action"
            android:layout_weight="1"
            style="@style/CerebriMediumTextBody.Accent"/>
    </dOYHB6.lN8Dz0.d8NVX3>
    <dOYHB6.lN8Dz0.d8NVX3
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/layout_confirm_personal_info_birth_certificate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:weightSum="4">
        <TextView
            android:gravity="start"
            android:id="@+id/text_view_label_birth_certificate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/maya_kyc_personal_info_confirm_submission_birth_certificate_label"
            android:layout_weight="3"
            style="@style/CerebriBook"/>
        <ImageView
            android:layout_gravity="end"
            android:id="@+id/image_view_birth_certificate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="0x7f080302"
            android:scaleType="fitCenter"
            android:adjustViewBounds="true"
            android:layout_weight="1"/>
    </dOYHB6.lN8Dz0.d8NVX3>
    <dOYHB6.lN8Dz0.d8NVX3
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/layout_confirm_personal_info_parent_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:weightSum="4">
        <TextView
            android:gravity="start"
            android:id="@+id/text_view_label_parent_id"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/maya_kyc_personal_info_confirm_submission_parent_primary_id_label"
            android:layout_weight="3"
            style="@style/CerebriBook"/>
        <ImageView
            android:layout_gravity="end"
            android:id="@+id/image_view_parent_id"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="0x7f080302"
            android:scaleType="fitCenter"
            android:adjustViewBounds="true"
            android:layout_weight="1"/>
    </dOYHB6.lN8Dz0.d8NVX3>
    <dOYHB6.lN8Dz0.d8NVX3
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/layout_confirm_personal_info_parental_consent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:weightSum="4">
        <TextView
            android:gravity="start"
            android:id="@+id/text_view_label_parental_consent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/maya_kyc_personal_info_confirm_submission_parent_consent_label"
            android:layout_weight="3"
            style="@style/CerebriBook"/>
        <ImageView
            android:layout_gravity="end"
            android:id="@+id/image_view_parental_consent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="0x7f080302"
            android:scaleType="fitCenter"
            android:adjustViewBounds="true"
            android:layout_weight="1"/>
    </dOYHB6.lN8Dz0.d8NVX3>
</androidx.constraintlayout.widget.ConstraintLayout>
