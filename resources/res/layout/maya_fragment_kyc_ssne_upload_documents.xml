<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/ghost_white"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView
        android:scrollbarThumbVertical="@drawable/maya_bg_scrollbar_grey_quaternary"
        android:scrollbarStyle="outsideOverlay"
        android:id="@+id/nested_scroll_view_form_container"
        android:paddingLeft="@dimen/maya_margin_large"
        android:paddingRight="@dimen/maya_margin_large"
        android:scrollbars="vertical"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/maya_margin_normal"
        android:overScrollMode="never"
        android:paddingHorizontal="@dimen/maya_margin_large"
        app:layout_constraintBottom_toTopOf="@+id/button_cancel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <dOYHB6.lN8Dz0.d8NVX3
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/image_view_ssne_upload"
                android:layout_width="@dimen/maya_kyc_icon_size_medium"
                android:layout_height="@dimen/maya_kyc_icon_size_medium"
                android:src="0x7f0802d0"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                app:layout_constraintBottom_toTopOf="@+id/text_view_title"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/text_view_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/maya_kyc_ssne_statement_header"
                style="@style/CerebriBoldH4.Primary"/>
            <TextView
                android:id="@+id/text_view_sub_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_small"
                android:text="@string/maya_kyc_ssne_statement_description"
                style="@style/CerebriBookLongRead.Gray"/>
            <TextView
                android:id="@+id/text_view_why_needed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_medium"
                android:layout_marginBottom="@dimen/maya_margin_medium"
                android:text="@string/maya_kyc_ssne_statement_why_needed"
                android:layout_marginVertical="@dimen/maya_margin_medium"
                style="@style/CerebriMediumLongRead.Accent"/>
            <TextView
                android:id="@+id/text_view_next_step_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_small"
                android:text="@string/maya_kyc_ssne_statement_next_steps_header"
                style="@style/CerebriOverlineBig.Primary"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint_layout_steps_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_medium">
                <TextView
                    android:gravity="center"
                    android:id="@+id/maya_kyc_first_step_text_view"
                    android:background="@drawable/maya_bg_content_information_oval_green"
                    android:padding="@dimen/maya_margin_2x_small"
                    android:layout_width="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:layout_height="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:text="@string/maya_kyc_ssne_statement_first_step_icon_text"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintBaseline_toBaselineOf="@+id/maya_kyc_first_step_content_text_view"
                    app:layout_constraintEnd_toStartOf="@+id/maya_kyc_first_step_content_text_view"
                    app:layout_constraintStart_toStartOf="parent"
                    style="@style/JekoBoldTextBody.Accent"/>
                <TextView
                    android:id="@+id/maya_kyc_first_step_content_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/maya_margin_medium"
                    android:text="@string/maya_kyc_ssne_statement_step_1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/maya_kyc_first_step_text_view"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/CerebriBookLongRead.Primary"/>
                <dOYHB6.lN8Dz0.d8NVX3
                    android:orientation="horizontal"
                    android:id="@+id/maya_kyc_first_step_sub_content_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="@+id/maya_kyc_first_step_content_text_view"
                    app:layout_constraintTop_toBottomOf="@+id/maya_kyc_first_step_content_text_view">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/maya_ic_download_green"
                        android:adjustViewBounds="true"
                        android:contentDescription="@null"
                        android:paddingStart="0dp"
                        android:paddingEnd="@dimen/maya_padding_5dp"/>
                    <TextView
                        android:textSize="@dimen/maya_text_size_small_text"
                        android:id="@+id/text_view_ssne_statement_download_link"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/maya_kyc_ssne_statement_step_1_5"
                        style="@style/CerebriBookLongRead.Accent"/>
                </dOYHB6.lN8Dz0.d8NVX3>
                <View
                    android:id="@+id/maya_kyc_first_to_second_dotted_image_view"
                    android:background="@drawable/maya_custom_drawable_dotted_line_vertical_green"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toTopOf="@+id/maya_kyc_second_step_text_view"
                    app:layout_constraintEnd_toEndOf="@+id/maya_kyc_first_step_text_view"
                    app:layout_constraintStart_toStartOf="@+id/maya_kyc_first_step_text_view"
                    app:layout_constraintTop_toBottomOf="@+id/maya_kyc_first_step_text_view"/>
                <TextView
                    android:gravity="center"
                    android:id="@+id/maya_kyc_second_step_text_view"
                    android:background="@drawable/maya_bg_content_information_green"
                    android:padding="@dimen/maya_padding_2x_small"
                    android:layout_width="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:layout_height="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:layout_marginTop="@dimen/maya_margin_medium"
                    android:text="@string/maya_kyc_ssne_statement_second_step_icon_text"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintEnd_toStartOf="@+id/maya_kyc_second_step_content_text_view"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/maya_kyc_first_step_sub_content_text_view"
                    style="@style/JekoBoldTextBody.Accent"/>
                <TextView
                    android:id="@+id/maya_kyc_second_step_content_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_kyc_ssne_statement_step_2"
                    app:layout_constraintBottom_toBottomOf="@+id/maya_kyc_second_step_text_view"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/maya_kyc_second_step_text_view"
                    app:layout_constraintTop_toTopOf="@+id/maya_kyc_second_step_text_view"
                    style="@style/CerebriBookLongRead.Primary"/>
                <View
                    android:id="@+id/maya_kyc_second_to_third_dotted_image_view"
                    android:background="@drawable/maya_custom_drawable_dotted_line_vertical_green"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toTopOf="@+id/maya_kyc_third_step_text_view"
                    app:layout_constraintEnd_toEndOf="@+id/maya_kyc_second_step_text_view"
                    app:layout_constraintStart_toStartOf="@+id/maya_kyc_second_step_text_view"
                    app:layout_constraintTop_toBottomOf="@+id/maya_kyc_second_step_text_view"/>
                <TextView
                    android:gravity="center"
                    android:id="@+id/maya_kyc_third_step_text_view"
                    android:background="@drawable/maya_bg_content_information_green"
                    android:padding="@dimen/maya_padding_2x_small"
                    android:layout_width="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:layout_height="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:layout_marginTop="@dimen/maya_margin_large"
                    android:text="@string/maya_kyc_ssne_statement_third_step_icon_text"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintEnd_toStartOf="@+id/maya_kyc_third_step_content_text_view"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/maya_kyc_second_step_content_text_view"
                    style="@style/JekoBoldTextBody.Accent"/>
                <TextView
                    android:id="@+id/maya_kyc_third_step_content_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_kyc_ssne_statement_step_3"
                    app:layout_constraintBottom_toBottomOf="@+id/maya_kyc_third_step_text_view"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/maya_kyc_third_step_text_view"
                    app:layout_constraintTop_toTopOf="@+id/maya_kyc_third_step_text_view"
                    style="@style/CerebriBookLongRead.Primary"/>
                <View
                    android:id="@+id/maya_kyc_third_to_fourth_dotted_image_view"
                    android:background="@drawable/maya_custom_drawable_dotted_line_vertical_green"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toTopOf="@+id/maya_kyc_fourth_step_text_view"
                    app:layout_constraintEnd_toEndOf="@+id/maya_kyc_third_step_text_view"
                    app:layout_constraintStart_toStartOf="@+id/maya_kyc_third_step_text_view"
                    app:layout_constraintTop_toBottomOf="@+id/maya_kyc_third_step_text_view"/>
                <TextView
                    android:gravity="center"
                    android:id="@+id/maya_kyc_fourth_step_text_view"
                    android:background="@drawable/maya_bg_content_information_green"
                    android:padding="@dimen/maya_padding_2x_small"
                    android:layout_width="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:layout_height="@dimen/maya_kyc_complete_your_maya_experience_step_icon_size"
                    android:layout_marginTop="@dimen/maya_margin_large"
                    android:text="@string/maya_kyc_ssne_statement_fourth_step_icon_text"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintEnd_toStartOf="@+id/maya_kyc_fourth_step_content_text_view"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/maya_kyc_third_step_content_text_view"
                    style="@style/JekoBoldTextBody.Accent"/>
                <TextView
                    android:id="@+id/maya_kyc_fourth_step_content_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_kyc_ssne_statement_step_4"
                    app:layout_constraintBottom_toBottomOf="@+id/maya_kyc_fourth_step_text_view"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/maya_kyc_fourth_step_text_view"
                    app:layout_constraintTop_toTopOf="@+id/maya_kyc_fourth_step_text_view"
                    style="@style/CerebriBookLongRead.Primary"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <com.paymaya.mayaui.kyczoloz.view.widget.KycMayaInfoSection
                android:id="@+id/maya_info_section"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/maya_margin_extra_large"
                app:kisDescriptionText="@string/maya_kyc_ssne_statement_restart_application"
                app:kisHeaderText="@string/maya_kyc_ssne_statement_why_need_header"
                app:kisIsClickable="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                style="@style/KycMayaInfoSectionDnfbp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/maya_almc_bottom_buttons_spacer"/>
        </dOYHB6.lN8Dz0.d8NVX3>
    </androidx.core.widget.NestedScrollView>
    <Button
        android:id="@+id/button_cancel"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/maya_margin_normal"
        android:text="@string/maya_kyc_ssne_statement_do_later_button"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_confirm"
        app:layout_constraintStart_toStartOf="parent"
        style="@style/MayaButtonSecondaryLarge.Cerebri"/>
    <Button
        android:id="@+id/button_confirm"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/maya_kyc_ssne_statement_upload_now_button"
        android:layout_marginStart="@dimen/maya_margin_small"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintBottom_toBottomOf="@+id/button_cancel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_cancel"
        style="@style/MayaButtonPrimaryLarge.Cerebri"/>
</androidx.constraintlayout.widget.ConstraintLayout>
