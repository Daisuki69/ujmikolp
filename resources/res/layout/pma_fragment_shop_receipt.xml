<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_pma_fragment_shop_receipt"
    android:paddingLeft="@dimen/pma_padding_extra_large"
    android:paddingTop="@dimen/pma_padding_normal"
    android:paddingRight="@dimen/pma_padding_extra_large"
    android:paddingBottom="@dimen/pma_padding_normal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.Guideline
        android:orientation="vertical"
        android:id="@+id/label_guide_pma_fragment_shop_receipt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.44"/>
    <include
        android:id="@+id/treats_coupon_container_view_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/pma_view_treats_coupon_shop_receipt"/>
    <include
        android:id="@+id/order_coupon_container_view_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/treats_coupon_container_view_pma_fragment_shop_receipt"
        layout="@layout/pma_view_order_coupon_shop_receipt"/>
    <dOYHB6.lN8Dz0.zvMGC1
        android:id="@+id/product_container_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/order_coupon_container_view_pma_fragment_shop_receipt">
        <dOYHB6.lN8Dz0.zvMGC1
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/pma_margin_small"
            android:layout_toLeftOf="@+id/product_image_view_pma_fragment_shop_receipt"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/pma_margin_small"
            android:layout_toStartOf="@+id/product_image_view_pma_fragment_shop_receipt"
            android:layout_alignParentStart="true">
            <TextView
                android:id="@+id/product_name_label_text_pma_fragment_shop_receipt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/pma_label_product_name"
                style="@style/Body2DarkLight"/>
            <TextView
                android:id="@+id/product_name_text_pma_fragment_shop_receipt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/pma_margin_2x_small"
                android:layout_below="@+id/product_name_label_text_pma_fragment_shop_receipt"
                style="@style/BodyDarkBold"/>
        </dOYHB6.lN8Dz0.zvMGC1>
        <ImageView
            android:id="@+id/product_image_view_pma_fragment_shop_receipt"
            android:background="@drawable/pma_circle_border"
            android:padding="@dimen/pma_shop_receipt_product_icon_image_padding"
            android:layout_width="@dimen/pma_shop_receipt_product_image_size"
            android:layout_height="@dimen/pma_shop_receipt_product_image_size"
            android:scaleType="fitCenter"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"/>
    </dOYHB6.lN8Dz0.zvMGC1>
    <View
        android:id="@+id/product_divider_pma_fragment_shop_receipt"
        android:background="@drawable/pma_broken_line_receipt_divider"
        android:layout_width="0dp"
        android:layout_height="@dimen/pma_height_divider_large"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:layerType="software"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/product_container_pma_fragment_shop_receipt"/>
    <TextView
        android:id="@+id/amount_title_text_pma_fragment_shop_receipt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:text="@string/pma_label_amount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/product_divider_pma_fragment_shop_receipt"
        app:layout_goneMarginTop="0dp"
        style="@style/Body2DarkLight"/>
    <TextView
        android:textSize="@dimen/pma_shop_receipt_amount_text_size"
        android:textColor="@color/error"
        android:id="@+id/amount_text_pma_fragment_shop_receipt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_2x_small"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amount_title_text_pma_fragment_shop_receipt"
        style="@style/BodyDarkBold"/>
    <TextView
        android:id="@+id/transaction_status_note_text_view_pma_fragment_shop_receipt"
        android:background="@drawable/pma_drawable_completed_button"
        android:padding="@dimen/pma_padding_extra_small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/pma_label_completed"
        android:drawableLeft="@drawable/pma_ic_check_status"
        android:drawablePadding="@dimen/pma_padding_extra_small"
        android:drawableStart="@drawable/pma_ic_check_status"
        app:layout_constraintBottom_toBottomOf="@+id/amount_text_pma_fragment_shop_receipt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/amount_title_text_pma_fragment_shop_receipt"
        style="@style/CaptionDarkLightBold"/>
    <TextView
        android:id="@+id/message_text_pma_fragment_shop_receipt"
        android:background="@drawable/pma_receipt_message_background"
        android:padding="@dimen/pma_padding_medium"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_medium"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amount_text_pma_fragment_shop_receipt"
        style="@style/Body2Dark"/>
    <View
        android:id="@+id/amount_divider_pma_fragment_shop_receipt"
        android:background="@drawable/pma_broken_line_receipt_divider"
        android:layout_width="0dp"
        android:layout_height="@dimen/pma_height_divider_large"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:layerType="software"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/message_text_pma_fragment_shop_receipt"
        app:layout_goneMarginTop="@dimen/pma_margin_normal"/>
    <TextView
        android:id="@+id/transaction_details_title_text_pma_fragment_shop_receipt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:text="@string/pma_label_transaction_details"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amount_divider_pma_fragment_shop_receipt"
        style="@style/BodyDarkMedium"/>
    <TextView
        android:id="@+id/transaction_details_product_name_label_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:text="@string/pma_label_product_name"
        app:layout_constraintEnd_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/transaction_details_title_text_pma_fragment_shop_receipt"
        style="@style/Body2DarkLight"/>
    <TextView
        android:ellipsize="end"
        android:id="@+id/transaction_details_product_name_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintTop_toTopOf="@+id/transaction_details_product_name_label_text_pma_fragment_shop_receipt"
        style="@style/Body2DarkBold"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/transaction_details_product_name_barrier_pma_fragment_shop_receipt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="transaction_details_product_name_label_text_pma_fragment_shop_receipt,                                                transaction_details_product_name_text_pma_fragment_shop_receipt"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/transaction_details_product_name_group_pma_fragment_shop_receipt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:constraint_referenced_ids="transaction_details_product_name_text_pma_fragment_shop_receipt,                                                transaction_details_product_name_label_text_pma_fragment_shop_receipt"/>
    <TextView
        android:id="@+id/recipient_label_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:text="@string/pma_label_recipient"
        app:layout_constraintEnd_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/transaction_details_product_name_barrier_pma_fragment_shop_receipt"
        style="@style/Body2DarkLight"/>
    <TextView
        android:id="@+id/recipient_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintTop_toTopOf="@+id/recipient_label_text_pma_fragment_shop_receipt"
        style="@style/Body2DarkBold"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/recipient_group_pma_fragment_shop_receipt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:constraint_referenced_ids="recipient_label_text_pma_fragment_shop_receipt,                                                recipient_text_pma_fragment_shop_receipt"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/recipient_barrier_pma_fragment_shop_recipient"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="recipient_label_text_pma_fragment_shop_receipt,                                                recipient_text_pma_fragment_shop_receipt"/>
    <TextView
        android:id="@+id/fund_source_label_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:text="@string/pma_label_fund_source"
        app:layout_constraintEnd_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recipient_barrier_pma_fragment_shop_recipient"
        style="@style/Body2DarkLight"/>
    <TextView
        android:id="@+id/fund_source_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintTop_toTopOf="@+id/fund_source_label_text_pma_fragment_shop_receipt"
        style="@style/Body2DarkBold"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/fund_source_group_pma_fragment_shop_receipt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:constraint_referenced_ids="fund_source_label_text_pma_fragment_shop_receipt,                                                fund_source_text_pma_fragment_shop_receipt"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/fund_source_barrier_pma_fragment_shop_receipt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="fund_source_label_text_pma_fragment_shop_receipt,                                                fund_source_text_pma_fragment_shop_receipt"/>
    <TextView
        android:id="@+id/payment_reference_id_label_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:text="@string/pma_label_payment_reference_id"
        app:layout_constraintEnd_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fund_source_barrier_pma_fragment_shop_receipt"
        style="@style/Body2DarkLight"/>
    <TextView
        android:id="@+id/payment_reference_id_text_pma_fragment_shop_receipt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/label_guide_pma_fragment_shop_receipt"
        app:layout_constraintTop_toTopOf="@+id/payment_reference_id_label_text_pma_fragment_shop_receipt"
        style="@style/Body2DarkBold"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/payment_reference_id_barrier_pma_fragment_shop_receipt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:constraint_referenced_ids="payment_reference_id_label_text_pma_fragment_shop_receipt,                                                payment_reference_id_text_pma_fragment_shop_receipt"/>
</androidx.constraintlayout.widget.ConstraintLayout>
