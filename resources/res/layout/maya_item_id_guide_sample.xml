<?xml version="1.0" encoding="utf-8"?>
<dOYHB6.lN8Dz0.d8NVX3 xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:paddingTop="@dimen/maya_margin_large"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/maya_margin_large"
    android:paddingEnd="@dimen/maya_margin_large">
    <TextView
        android:id="@+id/text_view_variant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/maya_margin_extra_small"
        android:maxLines="2"
        style="@style/CerebriMediumSubline.Primary"/>
    <com.paymaya.common.widget.HtmlTextView
        android:textColor="@color/davy_gray"
        android:textColorLink="@color/pma_primary_100_base"
        android:id="@+id/text_view_description"
        android:clickable="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:htmlUnderline="false"
        style="@style/CerebriBookLink"/>
    <dOYHB6.lN8Dz0.d8NVX3
        android:orientation="vertical"
        android:id="@+id/linear_layout_images"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_normal"/>
    <com.paymaya.common.widget.HtmlTextView
        android:textColor="@color/davy_gray"
        android:textColorLink="@color/pma_primary_100_base"
        android:id="@+id/text_view_description_secondary"
        android:visibility="gone"
        android:clickable="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_normal"
        app:htmlUnderline="false"
        style="@style/CerebriBookLink"/>
    <View
        android:id="@+id/view_sample_divider"
        android:background="@color/bright_gray"
        android:layout_width="match_parent"
        android:layout_height="@dimen/maya_stroke_width_small"
        android:layout_marginTop="@dimen/maya_margin_large"/>
</dOYHB6.lN8Dz0.d8NVX3>
