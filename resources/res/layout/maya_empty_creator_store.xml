<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_fragment_creator_store_empty"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:id="@+id/start_guide_creator_store"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_begin="@dimen/maya_margin_48"/>
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:id="@+id/end_guide_creator_store"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_end="@dimen/maya_margin_48"/>
        <ImageView
            android:id="@+id/image_view_missed_target"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/maya_margin_110"
            android:contentDescription="@null"
            app:layout_constraintBottom_toTopOf="@+id/text_view_empty_store_title"
            app:layout_constraintEnd_toEndOf="@+id/end_guide_creator_store"
            app:layout_constraintStart_toStartOf="@+id/start_guide_creator_store"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed"
            app:srcCompat="0x7f0802e8"/>
        <TextView
            android:gravity="center"
            android:id="@+id/text_view_empty_store_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/maya_label_no_featured_partners_available"
            app:layout_constraintBottom_toTopOf="@+id/text_view_fragment_online_payment_empty"
            app:layout_constraintEnd_toEndOf="@+id/end_guide_creator_store"
            app:layout_constraintStart_toStartOf="@+id/start_guide_creator_store"
            app:layout_constraintTop_toBottomOf="@+id/image_view_missed_target"
            style="@style/JekoBoldH5.Primary"/>
        <TextView
            android:gravity="center"
            android:id="@+id/text_view_fragment_online_payment_empty"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/maya_margin_small"
            android:text="@string/maya_label_no_partners_available_message"
            app:layout_constraintEnd_toEndOf="@+id/end_guide_creator_store"
            app:layout_constraintStart_toStartOf="@+id/start_guide_creator_store"
            app:layout_constraintTop_toBottomOf="@+id/text_view_empty_store_title"
            style="@style/CerebriBookLongRead.Secondary"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>
