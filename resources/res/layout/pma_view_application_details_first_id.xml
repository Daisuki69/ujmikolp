<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_view_group_pma_view_application_details_first_id"
    android:paddingBottom="@dimen/pma_padding_normal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.Guideline
        android:orientation="vertical"
        android:id="@+id/guideline_pma_view_application_details_first_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.45"/>
    <TextView
        android:id="@+id/id_type_label_text_view_pma_view_application_details_first_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/pma_margin_small"
        android:text="@string/pma_label_id_type"
        android:layout_marginEnd="@dimen/pma_margin_small"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/BodyDarkLight"/>
    <TextView
        android:id="@+id/id_type_text_view_pma_view_application_details_first_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pma_margin_small"
        android:layout_marginStart="@dimen/pma_margin_small"
        app:layout_constraintLeft_toRightOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/BodyDark"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/id_type_barrier_pma_view_application_details_first_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="id_type_text_view_pma_view_application_details_first_id,             id_type_label_text_view_pma_view_application_details_first_id"/>
    <TextView
        android:id="@+id/id_number_label_text_view_pma_view_application_details_first_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:layout_marginRight="@dimen/pma_margin_small"
        android:text="@string/pma_label_id_number"
        android:layout_marginEnd="@dimen/pma_margin_small"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintTop_toBottomOf="@+id/id_type_barrier_pma_view_application_details_first_id"
        style="@style/BodyDarkLight"/>
    <TextView
        android:id="@+id/id_number_text_view_pma_view_application_details_first_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pma_margin_small"
        android:layout_marginStart="@dimen/pma_margin_small"
        app:layout_constraintLeft_toRightOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/id_number_label_text_view_pma_view_application_details_first_id"
        style="@style/BodyDark"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/id_number_barrier_pma_view_application_details_first_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="id_number_label_text_view_pma_view_application_details_first_id,             id_number_text_view_pma_view_application_details_first_id"/>
    <TextView
        android:id="@+id/id_expiration_date_label_text_view_pma_view_application_details_first_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:layout_marginRight="@dimen/pma_margin_small"
        android:text="@string/pma_label_id_expiration_date"
        android:layout_marginEnd="@dimen/pma_margin_small"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintTop_toBottomOf="@+id/id_number_barrier_pma_view_application_details_first_id"
        style="@style/BodyDarkLight"/>
    <TextView
        android:id="@+id/id_expiration_date_text_view_pma_view_application_details_first_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pma_margin_small"
        android:layout_marginStart="@dimen/pma_margin_small"
        app:layout_constraintLeft_toRightOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/id_expiration_date_label_text_view_pma_view_application_details_first_id"
        style="@style/BodyDark"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/id_expiration_date_barrier_pma_view_application_details_first_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="id_expiration_date_label_text_view_pma_view_application_details_first_id,             id_expiration_date_text_view_pma_view_application_details_first_id"/>
    <TextView
        android:id="@+id/id_photo_label_text_view_pma_view_application_details_first_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_normal"
        android:layout_marginRight="@dimen/pma_margin_small"
        android:text="@string/pma_label_id_photo"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintTop_toBottomOf="@+id/id_expiration_date_barrier_pma_view_application_details_first_id"
        style="@style/BodyDarkLight"/>
    <ImageView
        android:id="@+id/id_photo_image_view_pma_view_application_details_first_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pma_margin_small"
        android:scaleType="fitXY"
        android:adjustViewBounds="true"
        android:maxWidth="@dimen/pma_ekyc_photo_preview_max_width"
        android:maxHeight="@dimen/pma_ekyc_photo_preview_max_height"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toRightOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/id_photo_label_text_view_pma_view_application_details_first_id"/>
    <ImageView
        android:id="@+id/back_id_photo_image_view_pma_view_application_details_first_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pma_margin_medium"
        android:scaleType="fitXY"
        android:adjustViewBounds="true"
        android:maxWidth="@dimen/pma_ekyc_photo_preview_max_width"
        android:maxHeight="@dimen/pma_ekyc_photo_preview_max_height"
        android:layout_marginStart="@dimen/pma_margin_small"
        android:layout_marginEnd="@dimen/pma_margin_normal"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toRightOf="@+id/guideline_pma_view_application_details_first_id"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_photo_image_view_pma_view_application_details_first_id"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/id_photo_barrier_pma_view_application_details_first_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="id_photo_label_text_view_pma_view_application_details_first_id,             back_id_photo_image_view_pma_view_application_details_first_id"/>
</androidx.constraintlayout.widget.ConstraintLayout>
