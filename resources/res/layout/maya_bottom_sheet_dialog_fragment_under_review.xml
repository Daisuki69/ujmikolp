<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/maya_bg_default_bottom_dialog_sheet"
    android:paddingBottom="@dimen/pma_margin_extra_large"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/image_view_banner"
        android:layout_width="@dimen/maya_kyc_confirmation_image_size"
        android:layout_height="@dimen/maya_kyc_confirmation_image_size"
        android:layout_marginTop="@dimen/maya_margin_large"
        android:layout_marginBottom="@dimen/maya_margin_large"
        android:src="0x7f0802a3"
        android:importantForAccessibility="no"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/text_view_header"
        android:paddingBottom="@dimen/maya_margin_small"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:text="@string/maya_label_under_review_title"
        android:textAlignment="center"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintBottom_toTopOf="@+id/text_view_sub_header"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_view_banner"
        style="@style/JekoBoldH5.Primary"/>
    <TextView
        android:id="@+id/text_view_sub_header"
        android:paddingBottom="@dimen/pma_padding_40dp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:text="@string/maya_label_under_review_subtitle"
        android:textAlignment="center"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_view_header"
        style="@style/CerebriBookLink.Secondary"/>
    <Button
        android:id="@+id/button_action_primary"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/maya_margin_normal"
        android:text="@string/maya_label_view_status"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintEnd_toStartOf="@+id/button_action_secondary"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_view_sub_header"
        style="@style/MayaButtonSecondaryMedium"/>
    <Button
        android:id="@+id/button_action_secondary"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/maya_label_got_it"
        android:layout_marginStart="@dimen/maya_margin_small"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_action_primary"
        app:layout_constraintTop_toBottomOf="@+id/text_view_sub_header"
        style="@style/MayaButtonPrimaryMedium"/>
</androidx.constraintlayout.widget.ConstraintLayout>
