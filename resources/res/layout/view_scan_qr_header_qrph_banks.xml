<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.paymaya.common.widget.TextViewWithImagesKt
        android:textColor="@color/desert_storm_white"
        android:id="@+id/text_view_header_scan_qrph_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/maya_label_qrph_code"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tvwiTextAlignment="center_vertical"
        style="@style/JekoBoldH6"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/view_supported_qrph_bank_background"
        android:background="@drawable/maya_bg_supported_qr_rounded_white_85"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_medium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_view_header_scan_qrph_code">
        <ImageView
            android:id="@+id/image_view_supported_qrph_one"
            android:layout_width="@dimen/maya_qr_supported_qrph_banks_width"
            android:layout_height="@dimen/maya_qr_supported_qrph_banks_height"
            android:layout_marginTop="@dimen/maya_margin_extra_small"
            android:layout_marginBottom="@dimen/maya_margin_extra_small"
            android:src="0x7f0803a1"
            android:scaleType="centerInside"
            android:adjustViewBounds="true"
            android:layout_marginStart="@dimen/maya_margin_normal"
            android:layout_marginEnd="@dimen/maya_margin_3x_small"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/image_view_supported_qrph_two"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view_supported_qrph_bank_background"/>
        <ImageView
            android:id="@+id/image_view_supported_qrph_two"
            android:layout_width="@dimen/maya_qr_supported_qrph_banks_width"
            android:layout_height="@dimen/maya_qr_supported_qrph_banks_height"
            android:layout_marginTop="@dimen/maya_margin_extra_small"
            android:layout_marginBottom="@dimen/maya_margin_extra_small"
            android:src="0x7f08039f"
            android:layout_marginStart="@dimen/maya_margin_3x_small"
            android:layout_marginEnd="@dimen/maya_margin_3x_small"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/image_view_supported_qrph_three"
            app:layout_constraintStart_toEndOf="@+id/image_view_supported_qrph_one"
            app:layout_constraintTop_toTopOf="@+id/view_supported_qrph_bank_background"/>
        <ImageView
            android:id="@+id/image_view_supported_qrph_three"
            android:layout_width="@dimen/maya_qr_supported_qrph_banks_width"
            android:layout_height="@dimen/maya_qr_supported_qrph_banks_height"
            android:layout_marginTop="@dimen/maya_margin_extra_small"
            android:layout_marginBottom="@dimen/maya_margin_extra_small"
            android:src="0x7f0803a2"
            android:layout_marginStart="@dimen/maya_margin_3x_small"
            android:layout_marginEnd="@dimen/maya_margin_3x_small"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/image_view_supported_qrph_four"
            app:layout_constraintStart_toEndOf="@+id/image_view_supported_qrph_two"
            app:layout_constraintTop_toTopOf="@+id/view_supported_qrph_bank_background"/>
        <ImageView
            android:id="@+id/image_view_supported_qrph_four"
            android:layout_width="@dimen/maya_qr_supported_qrph_banks_width"
            android:layout_height="@dimen/maya_qr_supported_qrph_banks_height"
            android:layout_marginTop="@dimen/maya_margin_extra_small"
            android:layout_marginBottom="@dimen/maya_margin_extra_small"
            android:src="0x7f0803a0"
            android:layout_marginStart="@dimen/maya_margin_3x_small"
            android:layout_marginEnd="@dimen/maya_margin_normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/image_view_supported_qrph_three"
            app:layout_constraintTop_toTopOf="@+id/view_supported_qrph_bank_background"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
