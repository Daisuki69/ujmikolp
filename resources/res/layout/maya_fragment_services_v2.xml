<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/mayaColorBackgroundPrimary"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView
        android:scrollbarThumbVertical="@drawable/maya_bg_scrollbar_grey_quaternary"
        android:id="@+id/scroll_view_container"
        android:background="?attr/mayaColorBackgroundPrimary"
        android:scrollbars="vertical"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:fadeScrollbars="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/container"
            android:clipChildren="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="false"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <Space
                android:id="@+id/space_services_title_top_margin"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/maya_margin_40"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:id="@+id/text_view_services_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/maya_label_services"
                android:layout_marginStart="@dimen/maya_margin_large"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/space_services_title_top_margin"
                style="@style/JekoBoldH4.Primary"/>
            <View
                android:background="?attr/mayaColorBackgroundPrimaryAlt"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="@+id/recycler_view"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recycler_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/text_view_services_title"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint_layout_container_help_tools"
                android:background="@drawable/maya_bg_content_border_dashboard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/maya_margin_130"
                android:layout_marginStart="@dimen/maya_margin_large"
                android:layout_marginEnd="@dimen/maya_margin_large"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/recycler_view">
                <View
                    android:id="@+id/container_view_security_center"
                    android:focusable="true"
                    android:clickable="true"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_security_center"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_security_center"/>
                <androidx.constraintlayout.widget.Group
                    android:id="@+id/constraint_group_security_center"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:constraint_referenced_ids="container_view_security_center,                 image_view_icon_security_center,                 text_view_security_center,                 image_view_arrow_security_center"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/image_view_icon_security_center"
                    android:paddingTop="@dimen/maya_padding_18dp"
                    android:paddingBottom="@dimen/maya_padding_18dp"
                    android:layout_width="@dimen/maya_icon_size_length_default"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_security_center"
                    android:tint="?attr/mayaColorIconTint"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    android:paddingVertical="@dimen/maya_padding_18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/maya_ic_service_chat"
                    app:tint="?attr/mayaColorIconTint"/>
                <TextView
                    android:id="@+id/text_view_security_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_label_security_center"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_security_center"
                    app:layout_constraintLeft_toRightOf="@+id/image_view_icon_security_center"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_security_center"
                    style="@style/JekoSemiBoldTextBody.Primary"/>
                <ImageView
                    android:id="@+id/image_view_arrow_security_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_help_grey_arrow"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_security_center"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_security_center"/>
                <View
                    android:id="@+id/divider_security_center"
                    android:background="@color/maya_shades_of_grey_2"
                    android:layout_width="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/container_view_security_center"
                    style="@style/MayaContentViewDivider"/>
                <View
                    android:id="@+id/container_view_messages"
                    android:focusable="true"
                    android:clickable="true"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_messages"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_messages"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/image_view_icon_messages"
                    android:paddingTop="@dimen/maya_padding_18dp"
                    android:paddingBottom="@dimen/maya_padding_18dp"
                    android:layout_width="@dimen/maya_icon_size_length_default"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_chat"
                    android:tint="?attr/mayaColorIconTint"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    android:paddingVertical="@dimen/maya_padding_18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/image_view_icon_security_center"
                    app:srcCompat="@drawable/maya_ic_service_chat"
                    app:tint="?attr/mayaColorIconTint"/>
                <TextView
                    android:id="@+id/text_view_messages"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_label_chat_with_maya"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_messages"
                    app:layout_constraintLeft_toRightOf="@+id/image_view_icon_messages"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_messages"
                    style="@style/JekoSemiBoldTextBody.Primary"/>
                <ImageView
                    android:id="@+id/image_view_arrow_messages"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_help_grey_arrow"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_messages"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_messages"/>
                <View
                    android:id="@+id/divider_messages"
                    android:background="@color/maya_shades_of_grey_2"
                    android:layout_width="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/container_view_messages"
                    style="@style/MayaContentViewDivider"/>
                <androidx.constraintlayout.widget.Group
                    android:id="@+id/group_messages"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:constraint_referenced_ids="container_view_messages,                 image_view_icon_messages,                 text_view_messages,                 image_view_arrow_messages,                 divider_messages"/>
                <View
                    android:id="@+id/container_view_help_center"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_help_center"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_help_center"
                    app:layout_constraintVertical_bias="0"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/image_view_icon_help_center"
                    android:paddingTop="@dimen/maya_padding_18dp"
                    android:paddingBottom="@dimen/maya_padding_18dp"
                    android:layout_width="@dimen/maya_icon_size_length_default"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_information_outline"
                    android:tint="?attr/mayaColorIconTint"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    android:paddingVertical="@dimen/maya_padding_18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/image_view_icon_messages"
                    app:srcCompat="@drawable/maya_ic_information_outline"
                    app:tint="?attr/mayaColorIconTint"/>
                <TextView
                    android:id="@+id/text_view_help_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_label_help_center"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_help_center"
                    app:layout_constraintLeft_toRightOf="@+id/image_view_icon_help_center"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_help_center"
                    style="@style/JekoSemiBoldTextBody.Primary"/>
                <ImageView
                    android:id="@+id/image_view_arrow_help_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_help_grey_arrow"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_help_center"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_help_center"/>
                <View
                    android:id="@+id/divider_help_center"
                    android:background="@color/maya_shades_of_grey_2"
                    android:layout_width="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/container_view_help_center"
                    style="@style/MayaContentViewDivider"/>
                <View
                    android:id="@+id/container_view_my_profile"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_my_profile"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_my_profile"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/image_view_icon_my_profile"
                    android:paddingTop="@dimen/maya_padding_18dp"
                    android:paddingBottom="@dimen/maya_padding_18dp"
                    android:layout_width="@dimen/maya_icon_size_length_default"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_smiley"
                    android:tint="?attr/mayaColorIconTint"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    android:paddingVertical="@dimen/maya_padding_18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/image_view_icon_help_center"
                    app:srcCompat="@drawable/maya_ic_service_smiley"
                    app:tint="?attr/mayaColorIconTint"/>
                <TextView
                    android:id="@+id/text_view_my_profile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_label_my_profile"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_my_profile"
                    app:layout_constraintLeft_toRightOf="@+id/image_view_icon_my_profile"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_my_profile"
                    style="@style/JekoSemiBoldTextBody.Primary"/>
                <ImageView
                    android:id="@+id/image_view_arrow_my_profile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_help_grey_arrow"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_my_profile"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_my_profile"/>
                <View
                    android:id="@+id/divider_my_profile"
                    android:background="@color/maya_shades_of_grey_2"
                    android:layout_width="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/container_view_my_profile"
                    style="@style/MayaContentViewDivider"/>
                <View
                    android:id="@+id/container_view_settings"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_settings"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_settings"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/image_view_icon_settings"
                    android:paddingTop="@dimen/maya_padding_18dp"
                    android:paddingBottom="@dimen/maya_padding_18dp"
                    android:layout_width="@dimen/maya_icon_size_length_default"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_settings"
                    android:tint="?attr/mayaColorIconTint"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    android:paddingVertical="@dimen/maya_padding_18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/image_view_icon_my_profile"
                    app:srcCompat="@drawable/maya_ic_service_settings"
                    app:tint="?attr/mayaColorIconTint"/>
                <TextView
                    android:id="@+id/text_view_settings"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_label_settings"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_settings"
                    app:layout_constraintLeft_toRightOf="@+id/image_view_icon_settings"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_settings"
                    style="@style/JekoSemiBoldTextBody.Primary"/>
                <ImageView
                    android:id="@+id/image_view_arrow_settings"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_help_grey_arrow"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_settings"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_settings"/>
                <View
                    android:id="@+id/divider_settings"
                    android:background="@color/maya_shades_of_grey_2"
                    android:layout_width="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/container_view_settings"
                    style="@style/MayaContentViewDivider"/>
                <View
                    android:id="@+id/container_view_about"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_about"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider_settings"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_about"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/image_view_icon_about"
                    android:paddingTop="@dimen/maya_padding_18dp"
                    android:paddingBottom="@dimen/maya_padding_18dp"
                    android:layout_width="@dimen/maya_icon_size_length_default"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_about_maya_v2"
                    android:tint="?attr/mayaColorIconTint"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    android:paddingVertical="@dimen/maya_padding_18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/image_view_icon_settings"
                    app:srcCompat="@drawable/maya_ic_service_about_maya_v2"
                    app:tint="?attr/mayaColorIconTint"/>
                <TextView
                    android:id="@+id/text_view_about"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/maya_label_about_maya"
                    android:layout_marginStart="@dimen/maya_margin_normal"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_about"
                    app:layout_constraintLeft_toRightOf="@+id/image_view_icon_about"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_about"
                    style="@style/JekoSemiBoldTextBody.Primary"/>
                <ImageView
                    android:id="@+id/image_view_arrow_about"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/maya_ic_service_help_grey_arrow"
                    android:layout_marginEnd="@dimen/maya_margin_medium"
                    app:layout_constraintBottom_toBottomOf="@+id/image_view_icon_about"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/image_view_icon_about"/>
                <View
                    android:id="@+id/divider_about"
                    android:background="@color/maya_shades_of_grey_2"
                    android:layout_width="match_parent"
                    app:layout_constraintBottom_toBottomOf="@+id/container_view_about"
                    style="@style/MayaContentViewDivider"/>
                <include
                    android:id="@+id/maya_ads_widgets_service_layout"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider_about"
                    layout="@layout/maya_ads_widgets_service_layout"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
    <include
        android:id="@+id/toolbar"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_custom_toolbar"/>
</androidx.constraintlayout.widget.ConstraintLayout>
