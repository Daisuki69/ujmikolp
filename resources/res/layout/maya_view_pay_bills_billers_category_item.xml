<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/pay_bill_category_item_container_constraint"
    android:background="?attr/mayaContentViewBackgroundColor"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/pma_margin_small"
    android:paddingStart="@dimen/pma_padding_medium"
    android:paddingEnd="@dimen/pma_padding_medium">
    <dOYHB6.lN8Dz0.yf3D09
        android:id="@+id/pay_bill_category_item_icon_container_frame"
        android:background="@drawable/maya_bg_selector_image_button_grey"
        android:focusable="true"
        android:clickable="true"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:layout_gravity="center"
            android:id="@+id/pay_bill_category_item_icon_image_view"
            android:layout_width="@dimen/maya_pay_bill_category_icon_width"
            android:layout_height="wrap_content"
            android:scaleType="fitCenter"/>
    </dOYHB6.lN8Dz0.yf3D09>
    <TextView
        android:ellipsize="end"
        android:gravity="center_horizontal"
        android:id="@+id/pay_bill_category_item_title_text_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pma_margin_small"
        android:layout_marginTop="@dimen/pma_margin_small"
        android:layout_marginRight="@dimen/pma_margin_small"
        android:lines="3"
        android:lineSpacingExtra="0sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pay_bill_category_item_icon_container_frame"
        style="@style/JekoBoldSmallText.Tertiary"/>
</androidx.constraintlayout.widget.ConstraintLayout>
