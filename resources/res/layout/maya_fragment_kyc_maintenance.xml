<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/image_view_maintenance_image"
        android:layout_width="@dimen/maya_kyc_maintenance_image_size"
        android:layout_height="@dimen/maya_kyc_maintenance_image_size"
        android:src="0x7f0802db"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toTopOf="@+id/text_view_header"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:gravity="center"
        android:id="@+id/text_view_header"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginTop="@dimen/maya_margin_small"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:text="@string/maya_kyc_maintenance_header"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/image_view_maintenance_image"
        app:layout_constraintStart_toStartOf="@+id/image_view_maintenance_image"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/JekoBoldH5.Primary"/>
    <TextView
        android:gravity="center"
        android:id="@+id/text_view_header_subtext"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:text="@string/maya_kyc_maintenance_header_subtext"
        app:layout_constraintEnd_toEndOf="@+id/image_view_maintenance_image"
        app:layout_constraintStart_toStartOf="@+id/image_view_maintenance_image"
        app:layout_constraintTop_toBottomOf="@+id/text_view_header"
        style="@style/CerebriBookLink.Secondary"/>
</androidx.constraintlayout.widget.ConstraintLayout>
