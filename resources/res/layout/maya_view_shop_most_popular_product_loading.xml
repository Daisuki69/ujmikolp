<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_card_view_pma_view_shop_v3_buy_again_purchase_card_loading"
    android:background="?attr/mayaContentBorderCard"
    android:layout_width="@dimen/maya_shop_loading_width"
    android:layout_height="@dimen/maya_shop_loading_height">
    <include
        android:id="@+id/shop_buy_again_loading_top"
        android:layout_width="@dimen/maya_shop_buy_again_loading_top_size"
        android:layout_height="@dimen/maya_shop_buy_again_loading_top_size"
        android:layout_marginTop="@dimen/maya_margin_large"
        android:layout_marginStart="@dimen/maya_margin_medium"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_shimmer_rounded_2"/>
    <include
        android:id="@+id/shop_buy_again_loading_line_first"
        android:layout_width="@dimen/maya_shop_shop_buy_again_loading_line_first_width"
        android:layout_height="@dimen/maya_shop_shop_buy_again_loading_line_first_height"
        android:layout_marginTop="@dimen/maya_margin_medium"
        android:layout_marginStart="@dimen/maya_margin_medium"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shop_buy_again_loading_top"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/shop_buy_again_loading_line_second"
        android:layout_width="@dimen/maya_shop_shop_buy_again_loading_line_second_width"
        android:layout_height="@dimen/maya_shop_shop_buy_again_loading_line_second_height"
        android:layout_marginTop="@dimen/maya_margin_medium"
        android:layout_marginStart="@dimen/maya_margin_medium"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shop_buy_again_loading_line_first"
        layout="@layout/maya_view_shimmer_rounded"/>
    <View
        android:id="@+id/voucher_details_divider_1"
        android:background="?attr/mayaColorDividerBackgroundPrimary"
        android:layout_width="match_parent"
        android:layout_height="@dimen/maya_height_divider_normal"
        android:layout_marginLeft="@dimen/maya_margin_normal"
        android:layout_marginTop="@dimen/maya_margin_medium"
        android:layout_marginRight="@dimen/maya_margin_normal"
        android:layout_marginHorizontal="@dimen/maya_margin_normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shop_buy_again_loading_line_second"/>
    <include
        android:id="@+id/shop_buy_again_loading_line_third"
        android:layout_width="@dimen/maya_shop_shop_buy_again_loading_line_third_width"
        android:layout_height="@dimen/maya_shop_shop_buy_again_loading_line_third_height"
        android:layout_marginTop="@dimen/maya_margin_medium"
        android:layout_marginStart="@dimen/maya_margin_medium"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/voucher_details_divider_1"
        layout="@layout/maya_view_shimmer_rounded"/>
</androidx.constraintlayout.widget.ConstraintLayout>
