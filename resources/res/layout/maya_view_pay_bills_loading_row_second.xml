<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_card_view_pma_view_holder_favorite_card_loading"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/favourite_loading_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_pay_bills_loading_cell_second"/>
    <include
        android:id="@+id/favourite_loading_third"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintStart_toEndOf="@+id/favourite_loading_second"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_pay_bills_loading_cell_second"/>
    <include
        android:id="@+id/favourite_loading_fourth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintStart_toEndOf="@+id/favourite_loading_third"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_pay_bills_loading_cell_second"/>
    <include
        android:id="@+id/favourite_loading_fifth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/maya_margin_large"
        android:layout_marginRight="@dimen/maya_margin_large"
        android:layout_marginHorizontal="@dimen/maya_margin_large"
        app:layout_constraintStart_toEndOf="@+id/favourite_loading_fourth"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_pay_bills_loading_cell_second"/>
</androidx.constraintlayout.widget.ConstraintLayout>
