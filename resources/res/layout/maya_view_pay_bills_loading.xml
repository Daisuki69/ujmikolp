<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_card_view_pma_view_holder_favorite_card_loading"
    android:background="?attr/mayaColorBackgroundPrimary"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/favourite_loading_title"
        android:layout_width="@dimen/maya_favorite_loading_label_width"
        android:layout_height="@dimen/maya_favorite_loading_label_height"
        android:layout_marginTop="@dimen/maya_margin_28"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/favourite_loading_view_all"
        android:layout_width="@dimen/maya_favorite_loading_view_all_width"
        android:layout_height="@dimen/maya_favorite_loading_view_all_height"
        android:layout_marginStart="@dimen/maya_margin_medium"
        android:layout_marginEnd="@dimen/maya_margin_large"
        app:layout_constraintBottom_toBottomOf="@+id/favourite_loading_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/favourite_loading_title"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/favourite_loading_first_box"
        android:layout_width="@dimen/maya_favorite_loading_box_width"
        android:layout_height="@dimen/maya_favorite_loading_box_height"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favourite_loading_title"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/favourite_loading_second_box"
        android:layout_width="@dimen/maya_favorite_loading_box_width"
        android:layout_height="@dimen/maya_favorite_loading_box_height"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginStart="@dimen/maya_margin_normal"
        app:layout_constraintStart_toEndOf="@+id/favourite_loading_first_box"
        app:layout_constraintTop_toBottomOf="@+id/favourite_loading_title"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/favourite_loading_third_box"
        android:layout_width="@dimen/maya_favorite_loading_box_width"
        android:layout_height="@dimen/maya_favorite_loading_box_height"
        android:layout_marginTop="@dimen/maya_margin_normal"
        android:layout_marginStart="@dimen/maya_margin_normal"
        app:layout_constraintStart_toEndOf="@+id/favourite_loading_second_box"
        app:layout_constraintTop_toBottomOf="@+id/favourite_loading_title"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/favourite_loading_line"
        android:layout_width="@dimen/maya_favorite_loading_line_width"
        android:layout_height="@dimen/maya_favorite_loading_line_height"
        android:layout_marginTop="@dimen/maya_margin_30"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favourite_loading_second_box"
        layout="@layout/maya_view_shimmer_rounded"/>
    <include
        android:id="@+id/pay_bills_row_first"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_large"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintTop_toBottomOf="@+id/favourite_loading_line"
        layout="@layout/maya_view_pay_bills_loading_row"/>
    <include
        android:id="@+id/pay_bills_row_second"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_large"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintTop_toBottomOf="@+id/pay_bills_row_first"
        layout="@layout/maya_view_pay_bills_loading_row_second"/>
    <include
        android:id="@+id/pay_bills_row_third"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/maya_margin_large"
        android:layout_marginStart="@dimen/maya_margin_large"
        app:layout_constraintTop_toBottomOf="@+id/pay_bills_row_second"
        layout="@layout/maya_view_pay_bills_loading_row"/>
</androidx.constraintlayout.widget.ConstraintLayout>
