<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container_constraint_layout_pma_view_shop_v3_purchase_history_list_dynamic_field"
    android:paddingBottom="@dimen/pma_padding_small"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/label_text_view_pma_view_shop_v3_purchase_history_list_dynamic_field"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/Body2Dark"/>
    <TextView
        android:ellipsize="end"
        android:id="@+id/value_text_view_pma_view_shop_v3_purchase_history_list_dynamic_field"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pma_margin_extra_small"
        android:maxLines="2"
        app:layout_constraintBaseline_toBaselineOf="@+id/label_text_view_pma_view_shop_v3_purchase_history_list_dynamic_field"
        app:layout_constraintLeft_toRightOf="@+id/label_text_view_pma_view_shop_v3_purchase_history_list_dynamic_field"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/Body2Dark"/>
</androidx.constraintlayout.widget.ConstraintLayout>
