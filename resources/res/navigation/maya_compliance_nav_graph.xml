<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/maya_compliance_nav_graph"
    app:startDestination="@+id/maya_compliance_initial_loading_fragment">
    <fragment
        android:label="@string/maya_kyc_loader_title"
        android:name="com.paymaya.mayaui.compliance.view.fragment.impl.MayaComplianceInitialLoadingFragment"
        android:id="@+id/maya_compliance_initial_loading_fragment">
        <action
            android:id="@+id/action_to_mayaFatcaFragment"
            app:destination="@+id/maya_compliance_fatca_fragment"
            app:popUpTo="@+id/maya_compliance_initial_loading_fragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_to_mayaDosriFragment"
            app:destination="@+id/maya_compliance_dosri_fragment"
            app:popUpTo="@+id/maya_kyc_initial_loading_fragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_mayaComplianceInitialLoadingFragment_to_mayaKycErrorDialogFragment"
            app:destination="@+id/maya_compliance_error_dialog_fragment"/>
    </fragment>
    <fragment
        android:label="todo please change this"
        android:name="com.paymaya.mayaui.kyczoloz.view.fragment.impl.MayaKycFatcaFragment"
        android:id="@+id/maya_compliance_fatca_fragment">
        <action
            android:id="@+id/action_mayaComplianceFatcaFragment_to_mayaComplianceInitialLoadingFragment"
            app:destination="@+id/maya_compliance_initial_loading_fragment"/>
        <action
            android:id="@+id/action_to_kyc_error_dialog_fragment"
            app:destination="@+id/maya_compliance_error_dialog_fragment"/>
        <action
            android:id="@+id/action_to_maya_kyc_dosri_fragment"
            app:destination="@+id/maya_compliance_dosri_fragment"/>
    </fragment>
    <fragment
        android:name="com.paymaya.mayaui.kyczoloz.view.fragment.impl.MayaKycDosriFragment"
        android:id="@+id/maya_compliance_dosri_fragment">
        <action
            android:id="@+id/action_to_single_selection_bottom_sheet_dialog_fragment"
            app:destination="@+id/maya_compliance_single_selection_bottom_sheet_dialog_fragment"/>
    </fragment>
    <dialog
        android:label="@string/maya_kyc_error"
        android:name="com.paymaya.mayaui.kyczoloz.view.fragment.impl.MayaKycErrorDialogFragment"
        android:id="@+id/maya_compliance_error_dialog_fragment">
        <argument
            android:name="drawableRes"
            app:argType="integer"/>
        <argument
            android:name="titleRes"
            app:argType="integer"/>
        <argument
            android:name="messageRes"
            app:argType="integer"/>
        <argument
            android:name="actionRes"
            app:argType="integer"/>
        <argument
            android:name="pageSource"
            app:argType="string"/>
        <argument
            android:name="errorCode"
            app:argType="integer"/>
    </dialog>
    <dialog
        android:label="@string/maya_kyc_single_selection_work_nature"
        android:name="com.paymaya.mayaui.kyczoloz.view.fragment.impl.MayaKycSingleSelectionBottomSheetDialogFragment"
        android:id="@+id/maya_compliance_single_selection_bottom_sheet_dialog_fragment">
        <argument
            android:name="uiModel"
            app:argType="com.paymaya.domain.model.KycSelectionModel"/>
    </dialog>
</navigation>
