<?xml version="1.0" encoding="utf-8"?>
<dOYHB6.lN8Dz0.yf3D09 xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/header_frame_layout"
    android:layout_width="match_parent"
    android:layout_height="170dp">
    <dOYHB6.lN8Dz0.zvMGC1
        android:id="@+id/header_relative_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <dOYHB6.lN8Dz0.d8NVX3
            android:orientation="horizontal"
            android:id="@+id/header_linear_layout_1"
            android:focusable="true"
            android:layout_width="match_parent"
            android:layout_height="118dp">
            <ImageView
                android:id="@+id/header_icon"
                android:focusable="true"
                android:layout_width="78dp"
                android:layout_height="78dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:scaleType="fitCenter"
                android:contentDescription="@string/ct_inapp_img"/>
            <dOYHB6.lN8Dz0.d8NVX3
                android:orientation="vertical"
                android:id="@+id/header_linear_layout_2"
                android:layout_width="match_parent"
                android:layout_height="118dp">
                <TextView
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:id="@+id/header_title"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginStart="20dp"/>
                <TextView
                    android:textSize="14sp"
                    android:id="@+id/header_message"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="4dp"
                    android:maxLines="3"
                    android:layout_marginStart="20dp"/>
            </dOYHB6.lN8Dz0.d8NVX3>
        </dOYHB6.lN8Dz0.d8NVX3>
        <dOYHB6.lN8Dz0.d8NVX3
            android:orientation="horizontal"
            android:id="@+id/header_linear_layout_3"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:weightSum="2"
            android:layout_below="@+id/header_linear_layout_1">
            <Button
                android:textSize="14sp"
                android:id="@+id/header_button_1"
                android:padding="2dp"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
            <Button
                android:textSize="14sp"
                android:id="@+id/header_button_2"
                android:padding="2dp"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
        </dOYHB6.lN8Dz0.d8NVX3>
    </dOYHB6.lN8Dz0.zvMGC1>
</dOYHB6.lN8Dz0.yf3D09>
