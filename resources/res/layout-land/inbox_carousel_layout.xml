<?xml version="1.0" encoding="utf-8"?>
<dOYHB6.lN8Dz0.d8NVX3 xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <dOYHB6.lN8Dz0.zvMGC1
        android:orientation="vertical"
        android:id="@+id/body_linear_layout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:baselineAligned="false">
        <dOYHB6.lN8Dz0.d8NVX3
            android:id="@+id/carousel_linear_layout"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_above="@+id/timestamp_relative_layout">
            <com.clevertap.android.sdk.inbox.CTCarouselViewPager
                android:id="@+id/image_carousel_viewpager"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"/>
        </dOYHB6.lN8Dz0.d8NVX3>
        <dOYHB6.lN8Dz0.zvMGC1
            android:id="@+id/timestamp_relative_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true">
            <dOYHB6.lN8Dz0.d8NVX3
                android:orientation="horizontal"
                android:id="@+id/sliderDots"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:layout_centerHorizontal="true"/>
            <dOYHB6.lN8Dz0.d8NVX3
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">
                <TextView
                    android:textColor="@android:color/darker_gray"
                    android:id="@+id/carousel_timestamp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:layout_marginEnd="10dp"/>
                <ImageView
                    android:layout_gravity="center"
                    android:id="@+id/read_circle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/ct_read_circle"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="20dp"/>
            </dOYHB6.lN8Dz0.d8NVX3>
        </dOYHB6.lN8Dz0.zvMGC1>
    </dOYHB6.lN8Dz0.zvMGC1>
</dOYHB6.lN8Dz0.d8NVX3>
