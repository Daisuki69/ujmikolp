<?xml version="1.0" encoding="utf-8"?>
<dOYHB6.lN8Dz0.yf3D09 xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="bottom"
    android:id="@+id/footer_frame_layout"
    android:layout_width="match_parent"
    android:layout_height="170dp">
    <dOYHB6.lN8Dz0.zvMGC1
        android:id="@+id/footer_relative_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <dOYHB6.lN8Dz0.d8NVX3
            android:orientation="horizontal"
            android:id="@+id/footer_linear_layout_1"
            android:focusable="true"
            android:layout_width="match_parent"
            android:layout_height="118dp">
            <ImageView
                android:id="@+id/footer_icon"
                android:focusable="true"
                android:layout_width="78dp"
                android:layout_height="78dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:contentDescription="@string/ct_inapp_img"
                android:layout_marginStart="20dp"/>
            <dOYHB6.lN8Dz0.d8NVX3
                android:orientation="vertical"
                android:id="@+id/footer_linear_layout_2"
                android:layout_width="match_parent"
                android:layout_height="118dp"
                android:paddingStart="20dp"
                android:paddingEnd="20dp">
                <TextView
                    android:textSize="@dimen/txt_size_inapp_footer_title"
                    android:textStyle="bold"
                    android:id="@+id/footer_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"/>
                <TextView
                    android:textSize="@dimen/txt_size_inapp_footer_message"
                    android:id="@+id/footer_message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:maxLines="3"/>
            </dOYHB6.lN8Dz0.d8NVX3>
        </dOYHB6.lN8Dz0.d8NVX3>
        <dOYHB6.lN8Dz0.d8NVX3
            android:orientation="horizontal"
            android:id="@+id/footer_linear_layout_3"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:weightSum="2"
            android:layout_below="@+id/footer_linear_layout_1">
            <Button
                android:textSize="@dimen/txt_size_inapp_footer_button"
                android:id="@+id/footer_button_1"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
            <Button
                android:textSize="@dimen/txt_size_inapp_footer_button"
                android:id="@+id/footer_button_2"
                android:visibility="gone"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"/>
        </dOYHB6.lN8Dz0.d8NVX3>
    </dOYHB6.lN8Dz0.zvMGC1>
</dOYHB6.lN8Dz0.yf3D09>
