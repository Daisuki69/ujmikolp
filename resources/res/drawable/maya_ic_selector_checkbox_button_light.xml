<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true"
        android:drawable="@drawable/maya_ic_checkbox_state_pressed_light"/>
    <item
        android:state_enabled="true"
        android:state_checked="true"
        android:drawable="@drawable/maya_ic_checkbox_state_checked_light"/>
    <item
        android:state_enabled="false"
        android:state_checked="true"
        android:drawable="@drawable/maya_ic_checkbox_state_checked_disabled"/>
    <item android:drawable="@drawable/maya_ic_checkbox_state_default_light"/>
</selector>
