<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-300dp"
        android:right="-300dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="@dimen/maya_stroke_width_normal"
                    android:color="@color/neon_green_25_opacity"
                    android:dashWidth="@dimen/maya_stroke_width_large"
                    android:dashGap="@dimen/maya_stroke_width_large"/>
            </shape>
        </rotate>
    </item>
</layer-list>
